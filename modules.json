{
  "UI": {
    "range":["small_knob", "large_knob", "tuning_knob", "slider"],
    "switches":["momentary","n_switch"],
    "input":["signal_input","trigger"],
    "output":["signal_output","line_level"],
    "led":"led"    
  },

  "modules": {
      "comparator": {
        "A": "signal_input", "B": "signal_input", "max": "signal_output", "min":"signal_output"
      },
      "noise": {
        "noise":"signal_output"
      },
      "vca": {
        "in":"signal_input","cv":"signal_input","trim":"small_knob","bias":"small_knob"
      },
      "simpleLFO": {
        "rate":"small_knob", "sine":"signal_output","phasor":"signal_output","pulse":"signal_output"
      },
      "outs":{
        "in_1(mono)":"signal_input", "in_2(stereo)":"signal_input","volume":"large_knob","left":"signal_output","right":"signal_output"
      },
      "sample_and_hold":{
        "in":"signal_input","trigger":"signal_input","small_knob":"threshold"
      },
      "freeverb":{
        "feedback1":"small_knob","feedback2":"small_knob","damping":"small_knob","spread":"small_knob","in":"signal_input","out":"signal_output"
      }
  }


}