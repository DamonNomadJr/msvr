{
  "UI": {
    "range":["small_knob", "large_knob", "tuning_knob", "slider"],
    "switches":["momentary","n_switch"],
    "input":["inlet","trigger"],
    "output":["outlet","line_level"],
    "led":"led"    
  },

  "modules": {
    "sample_and_hold": {
        "_props":{ "kind":"sample_and_hold","pos": [0.0, 1.5, 0.0], "orient": [0, 0, 0, 1] },
        "signal":{"_props":{"kind":"inlet"}},
        "trigger":{"_props":{"kind":"inlet"}},
        "threshold":{ "_props":{"kind":"small_knob","range":[0,1],    "taper":"linear","value":0,"unit":"float"}},
        "out":{"_props":{"kind":"inlet"}}
    },
     
    "freevoib":{
        "_props":{ "kind":"freevoib","pos": [0.0, 1.5, 0.0], "orient": [0, 0, 0, 1] },
        "signal":{"_props":{"kind":"inlet"}},
        "feedback_1":{"_props":{"kind":"small_knob","range":[0,1],"taper":"linear","value":0.9,"unit":"float"}},
        "feedback_2":{"_props":{"kind":"small_knob","range":[0,1],"taper":"linear","value":0.5,"unit":"float"}},
        "damping":{"_props":{"kind":"small_knob","range":[0,1],"taper":"linear","value":0.5,"unit":"float"}},
        "spread":{"_props":{"kind":"small_knob","range":[0,400],"taper":"linear","value":100,"unit":"float"}},
        "out":{"_props":{"kind":"outlet"}}
    },
    
    "shifter":{
      "_props":{"kind":"shifter","pos": [0.0, 1.5, 0.0], "orient": [0, 0, 0, 1] },
      "signal":{"_props":{"kind":"inlet"}},
      "cv":{"_props":{"kind":"inlet"}},
      "range":{"_props":{"kind":"small_knob"}},
      "out":{"_props":{"kind":"outlet"}},
      ">":{"_props":{"kind":"outlet"}}
    },

    "constant":{
      "_props":{"kind":"constant","pos": [0.0, 1.5, 0.0], "orient": [0, 0, 0, 1] },
      "voltage":{"_props":{"kind":"large_knob"}},
      "out":{"_props":{"kind":"outlet"}}
    },

    "lfo":{
        "_props":{"kind":"lfo","pos": [0.0, 1.5, 0.0], "orient": [0, 0, 0, 1] },
        "fm_cv":{"_props":{"kind":"inlet"}},
        "phasor_sync":{"_props":{"kind":"inlet"}},
        "pulse_width_cv":{"_props":{"kind":"inlet"}},
        "rate":{"_props":{"kind":"large_knob","range":[0,80],"taper":"log 3.8","value":0.63,"unit":"Hz"}},
        "index":{"_props":{"kind":"small_knob","range":[0,10],"taper":"linear","value":1,"unit":"float"}},
        "pulse_width":{"_props":{"kind":"small_knob","range":[0,1],"taper":"linear","value":0.25,"unit":"float"}},
        "onset":{"_props":{"kind":"small_knob","range":[0,1],"taper":"linear","value":0,"unit":"float"}},
        "sine":{"_props":{"kind":"outlet"}},
        "phasor":{"_props":{"kind":"outlet"}},
        "pulse":{"_props":{"kind":"outlet"}},
        "sine_index":{"_props":{"kind":"outlet"}}
    },


    "dualvco":{
        "_props":{"kind":"dualvco","pos": [0.0, 0.5, 0.0], "orient": [0, 0, 0, 1] },
        "vco_1_cv":{"_props":{"kind":"inlet"}},
        "index_cv":{"_props":{"kind":"inlet"}},
        "vco_2_cv":{"_props":{"kind":"inlet"}},
        "feedback_cv":{"_props":{"kind":"inlet"}},
        "vco_1_rate":{"_props":{"kind":"large_knob","range":[0,6000],"taper":"log 3.8","value":120,"unit":"Hz"}},
        "vco_1_waveform": {"_props":{"kind":"n_switch","throws":["Sine","Phasor","Triangle"],"value":"Sine"}},
        "vco_2_rate":{"_props":{"kind":"large_knob","range":[0,6000],"taper":"log 3.8","value":3,"unit":"Hz"}},
        "vco_2_waveform": {"_props":{"kind":"n_switch","throws":["Sine","Phasor","Triangle"],"value":"Sine"}},
        "feedback":{"_props":{"kind":"small_knob","range":[0,6],"taper":"linear","value":0.25,"unit":"float"}},
        "vco_1":{"_props":{"kind":"outlet"}},
        "vco_2":{"_props":{"kind":"outlet"}},
        "master":{"_props":{"kind":"outlet"}}
    },

    "vca":{
        "_props":{"kind":"vca","pos": [0.0, 1.8, 0.0], "orient": [0, 0, 0, 1] },
        "signal":{"_props":{"kind":"inlet"}},
        "cv":{"_props":{"kind":"inlet"}},
        "cv_amount":{"_props":{"kind":"large_knob","range":[0,1],"taper":"linear","value":0.5,"unit":"float"}},
        "bias":{"_props":{"kind":"large_knob","range":[0,1],"taper":"linear","value":0.5,"unit":"float"}},
        "output":{"_props":{"kind":"outlet"}}
    },

    "comparator":{
        "_props":{"kind":"comparator","pos": [0.0, 0.8, 0.0], "orient": [0, 0, 0, 1] },
        "A":{"_props":{"kind":"inlet"}},
        "B":{"_props":{"kind":"inlet"}},
        "max":{"_props":{"kind":"outlet"}},
        "min":{"_props":{"kind":"outlet"}}
    },

    "outs":{
        "_props":{"kind":"outs","pos": [0.0, 0.8, 0.0], "orient": [0, 0, 0, 1] },
        "left_(mono)":{"_props":{"kind":"inlet"}},
        "right_(stereo)":{"_props":{"kind":"inlet"}},
        "volume":{"_props":{"kind":"small_knob","range":[0,1],"taper":"log 3.8","value":0.25,"unit":"float"}},
        "left":{"_props":{"kind":"outlet"}},
        "right":{"_props":{"kind":"outlet"}}
    },

    "noise":{
      "_props":{"kind":"noise","pos": [0.0, 0.8, 0.0], "orient": [0, 0, 0, 1] },
      "out":{"_props":{"kind":"outlet"}}
    }

  }
}